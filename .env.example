# Azure Blob Storage Configuration
# These variables are required for file uploads to work

# Your Azure Storage Account Name
REACT_APP_AZURE_STORAGE_ACCOUNT_NAME=your_storage_account_name

# Your Azure SAS Token (should start with 'sv=' or will be prefixed with '?')
REACT_APP_AZURE_SAS_TOKEN=your_sas_token_here

# Container name in Azure Blob Storage (defaults to 'uploads' if not specified)
REACT_APP_AZURE_CONTAINER_NAME=uploads
